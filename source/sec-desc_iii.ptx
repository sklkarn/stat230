<subsubsection xml:id="subsubsec-rng_cs">
  <title>Continuous Series</title>
  
  <p>
    <ol>
    <li>     
        <p>
          Exclusive Series
        </p>
        <p>
              <me>
        \begin{array}{c|c} 
       class &amp; f_i  \\ \hline 
